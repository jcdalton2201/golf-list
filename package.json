{
  "name": "golf-list",
  "version": "1.0.0",
  "description": "list of golf ",
  "main": "index.js",
  "scripts": {
    "cc": "node scripts/createComponent.js",
    "prebuild": "node ./scripts/injectTemplates.js",
    "build": "rollup -c --environment BUILD:dev",
    "test": "karma start test/karma.config.js",
    "watchStyle": "watch 'npm run build' src/",
    "start": "browser-sync start -c ./bs-config.js & npm run watchStyle",
    "workbox": "workbox generate:sw",
    "prebuild-docs": "npm run ugly",
    "build-docs": "node ./scripts/buildProd.js",
    "postbuild-docs": "workbox inject:manifest",
    "preugly": "npm run build",
    "sandbox": "browser-sync start -c ./sandbox-config.js",
    "ugly": "uglifyjs ./.temp/golf-web.js -o ./docs/golf-web.min.js --source-map"
  },
  "author": "Jason Dalton",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "crypto": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "fs": "0.0.1-security",
    "fs-extra": "^4.0.2",
    "glob-fs": "^0.1.7",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.2",
    "karma-webpack": "^3.0.5",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "proxy-middleware": "^0.15.0",
    "replacestream": "^4.0.3",
    "standard-version": "^5.0.2",
    "uglify-es": "^3.0.13",
    "watch": "^1.0.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "workbox-cli": "^2.0.3",
    "rollup": "^1.7.4",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-uglify-es": "0.0.1"
  },
  "dependencies": {
    "localforage": "^1.5.0",
    "workbox-sw": "^2.0.3"
  }
}
